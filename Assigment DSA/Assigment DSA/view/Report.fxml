<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableCell?>
<?import javafx.scene.control.Button?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReportController">
    <TableView fx:id="transactionTable">
        <TableColumn fx:id="descriptionColumn" text="Description"/>
        <TableColumn fx:id="amountColumn" text="Amount"/>
        <TableColumn fx:id="typeColumn" text="Type"/>
        <TableColumn fx:id="dateColumn" text="Date"/>
    </TableView>
    <Button text="Close Report" onAction="#handleCloseReport"/>
</VBox>
